  <header id="header">
    <div id="header-warp">
<% if(is_home()) { %>
      <h1 id="logo">
        <a href="/"><%=config.title%></a>
      </h1>
<% } else { %>
      <div id="logo">
        <a href="/"><%=config.title%></a>
      </div>
<% } %>
      <nav id="nav" class="clear">
<% for (var i in theme.menu){ %>
        <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
<% } %>
      </nav>
    </div>
  </header>
